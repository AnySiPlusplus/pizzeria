.row
  .col-sm-6.col-md-3
    .general-thumb-wrap
      .thumbnail.general-thumbnail
        %img.img-shadow.general-thumbnail-img{src: "https://i.postimg.cc/63Gn2tYs/9.png", alt: "design-book"}
        .thumb-hover
          .general-thumb-link-wrap
            %a.thumb-hover-link{href: "#"}
              %i.fa.fa-eye.thumb-icon{'aria-hidden': "true"}
            %a.thumb-hover-link{href: "#"}
              %i.fa.fa-shopping-cart.thumb-icon
    .mb-35
      .clearfix
        %p.pull-right.font-16.in-gold-500 €28.50
        %p.title.mr-55 Android Programming for Beginners
      %span.divider-sm
